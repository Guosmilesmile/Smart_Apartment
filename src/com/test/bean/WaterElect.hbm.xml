<?xml version="1.0"?>    
<!DOCTYPE hibernate-mapping PUBLIC     
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"    
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">    
<hibernate-mapping>    
    <class name="com.test.bean.WaterElect" table="we_waterelect" >  
        <id name="id" type="int" column="we_id">  
           <generator class="native"/>   
        </id>  
        <property name="housernumber" type="java.lang.String">
        	<column name="we_housernumber" length="11"/>
        </property>
        <property name="watermoney" type="java.lang.Integer">
        	<column name="we_watermoney" length="11"/>
        </property>
        <property name="electmoney" type="java.lang.Integer">
        	<column name="we_electmoney" length="11"/>
        </property>
        <property name="waternumber" type="java.lang.Integer">
        	<column name="we_waternumber" length="11"/>
        </property>
        <property name="electnumber" type="java.lang.Integer">
        	<column name="we_electnumber" length="11"/>
        </property>
    </class>  
</hibernate-mapping>  